{
    "name": "nestjs-structure-mobcoder",
    "version": "1.0.0",
    "description": "this is simple nest structure",
    "author": {"name" : "Pritam Raj"},
    "bin": {
        "mob-nestjs-structure": "mob-nestjs-structure.js"
      },
    "license": "UNLICENSED",
    "scripts": {
      "build": "tsc -p tsconfig.build.json",
      "debug": "nodemon",
      "format": "prettier --write \"src/**/*.ts\"",
      "start": "ts-node -r tsconfig-paths/register src/main.ts",
      "start:dev": "nodemon",
      "prestart:prod": "rimraf dist && npm run build",
      "start:prod": "node dist/main.js",
      "lint": "tslint --project ./tsconfig.json -e 'src/test/**/*.*'",
      "lint:fix": "tslint --project ./tsconfig.json -e 'src/test/**/*.*'  --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "keywords": [
      "nestjs_structure",
      "mobcoder_nestjs_structure",
      "typescript",
      "nestjs"
    ],
    "dependencies": {
      "@hapi/joi": "^17.1.1",
      "@nestjs/common": "^7.0.0",
      "@nestjs/core": "^7.0.0",
      "@nestjs/mongoose": "^7.0.2",
      "@nestjs/platform-express": "^7.0.0",
      "@nestjs/swagger": "^4.8.2",
      "class-transformer": "^0.2.3",
      "class-validator": "^0.12.2",
      "loader": "^2.1.1",
      "mongoose": "^5.9.22",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "rxjs": "^6.6.0",
      "swagger-ui-express": "^4.4.0",
      "tslint": "^6.1.2",
      "uuid": "^8.2.0"
    },
    
    "repository": {
      "type": "git",
      "url": "git+https://github.com/PritamRaj69/nestjs-structure-mobcoder.git"
    },
    "bugs": {
      "url": "https://github.com/PritamRaj69/nestjs-structure-mobcoder/issues"
    },
    "homepage": "https://github.com/PritamRaj69/nestjs-structure-mobcoder#readme"
  }
  